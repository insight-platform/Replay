{
  "management_port": 8080,
  "in_stream": {
    "url": "router+bind:ipc:///tmp/${SOCKET_PATH_IN:-undefined}",
    "receive_timeout": {
      "secs": 1,
      "nanos": 0
    },
    "receive_hwm": 1000,
    "topic_prefix_spec": {
      "source_id": "source_id"
    },
    "routing_ids_cache_size": 1000,
    "fix_ipc_permissions": 511
  },
  "out_stream": {
    "url": "dealer+connect:ipc:///tmp/${SOCKET_PATH_OUT:-undefined}",
    "send_timeout": {
      "secs": 1,
      "nanos": 0
    },
    "send_retries": 3,
    "receive_timeout": {
      "secs": 1,
      "nanos": 0
    },
    "receive_retries": 3,
    "send_hwm": 1000,
    "receive_hwm": 100,
    "inflight_ops": 100
  },
  "storage": {
    "rocksdb": {
      "path": "/tmp/rocksdb",
      "data_expiration_ttl": {
        "secs": 3600,
        "nanos": 0
      }
    }
  }
}
